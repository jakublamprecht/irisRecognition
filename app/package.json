{
  "name": "irisrecognition",
  "version": "1.0.0",
  "author": "Jakub Lamprecht",
  "description": "Masters thesis",
  "license": "ISC",
  "main": "./electron.main.js",
  "scripts": {
    "release:build": "concurrently --kill-others --names \"buildElectron,buildPython,bundle\" \"yarn build\" \"node packager/buildServer.js\"",
    "release:package:windows": "node packager/build.js --platform win",
    "release": "concurrently --kill-others --names \"build,package\" \"yarn release:build\" \"yarn release:package:windows\"",
    "app:start": "electron .",
    "build": "webpack --config webpack/webpack.prod.js",
    "build:dev": "webpack --config webpack/webpack.dev.js",
    "build:dev:watch": "webpack --config webpack/webpack.dev.js --watch",
    "dev": "concurrently --kill-others --names \"webpack,electron\" \"yarn build:dev:watch\" \"yarn app:start\""
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-styled-components": "^1.10.6",
    "clean-webpack-plugin": "^2.0.1",
    "commander": "^3.0.2",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-reload": "^1.4.0",
    "eslint": "^5.16.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@xstate/react": "^0.7.0",
    "antd": "^3.16.2",
    "axios": "^0.18.1",
    "electron": "^7.2.4",
    "electron-is-dev": "^1.1.0",
    "electron-packager": "^14.0.6",
    "python-shell": "^1.0.7",
    "qs": "^6.8.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-test-renderer": "^16.9.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.3.2",
    "xstate": "^4.6.7"
  }
}
